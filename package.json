{
	"name": "dartfileutils",
	"displayName": "DartFileUtils",
	"description": "A utils extension for dart files",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"dart",
		"flutter",
		"utils",
		"files"
	],
	"activationEvents": [
		"onCommand:dartfileutils.createTest"
	],
	"bugs": {
		"url": "https://github.com/defuncart/vscode-dartfileutils/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/defuncart/vscode-dartfileutils.git"
	},
	"homepage": "https://github.com/defuncart/vscode-dartfileutils/",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dartfileutils.createTest",
				"category": "Dart File Utils",
				"title": "Create Test"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == dart",
					"command": "dartfileutils.createTest",
					"group": "dartfileutils"
				}
			],
			"editor/title/context": [
				{
					"when": "resourceLangId == dart",
					"command": "dartfileutils.createTest",
					"group": "dartfileutils"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	}
}